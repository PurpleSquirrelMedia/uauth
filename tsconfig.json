{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["ESNext"],
    "allowJs": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true
  }
}
